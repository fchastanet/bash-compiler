  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch shellcheckLint Local",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/bash-compiler/main.go",
      "args": [
        "examples/configReference/shellcheckLint.yaml",
        "-t",
        "examples/generated",
        "-k",
        "-d"
      ],
      "cwd": "/home/wsl/fchastanet/bash-compiler",
      "env": {
        "FRAMEWORK_ROOT_DIR": "/home/wsl/fchastanet/bash-dev-env/vendor/bash-tools-framework"
      }
    },
    {
      "name": "Launch Bash-dev-env AwsDefaultConfig",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/bash-compiler/main.go",
      "args": [
        "/home/wsl/fchastanet/bash-dev-env/src/_binaries/AwsDefaultConfig/AwsDefaultConfig.yaml",
        "-t",
        "examples/generated",
        "-k"
      ],
      "cwd": "/home/wsl/fchastanet/bash-compiler",
      "env": {
        "FRAMEWORK_ROOT_DIR": "/home/wsl/fchastanet/bash-dev-env/vendor/bash-tools-framework",
        "BASH_DEV_ENV_ROOT_DIR": "/home/wsl/fchastanet/bash-dev-env"
      }
    }
  ]
}
